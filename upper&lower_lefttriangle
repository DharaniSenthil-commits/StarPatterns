*/

public class Pattern {

	public static void main(String args[]) {
		int n=5;
		for(int i=1;i<=n*2;i++) {
			if(i<=5) {
				for(int j=1;j<=i;j++) {
				System.out.print("*");
				}
				System.out.println();
			}
			else {
				for(int j=n*2-i;j>0;j--) {
					System.out.print("*");
				}
				System.out.println();
			}
		}
	}
}
